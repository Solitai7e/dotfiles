def_key "k"
  push_character "up"

def_key "j"
  push_character "down"

def_key "h"
  previous_column

def_key "l"
  next_column

def_key "h"
  jump_to_parent_directory

def_key "l"
  enter_directory

def_key "a"
  add_item_to_playlist

def_key "ctrl-d"
  push_character "page_down"

def_key "ctrl-u"
  push_character "page_up"

def_key "g"
  push_character "home"

def_key "shift-g"
  push_character "end"

def_key "space"
  select_item
  scroll_down

def_key "backspace"
  scroll_up
  select_item

def_key "d"
  push_character "delete"

def_key "-"
  volume_down

def_key "="
  volume_up